package com.briup.one2one;

public class Wife {
	private int id;
	private String name;
	private Hus hus;
	public Wife(){}
	public Wife(int id,String name,Hus hus){
		this.id = id;
		this.name = name;
		this.setHus(hus);
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Hus getHus() {
		return hus;
	}
	public void setHus(Hus hus) {
		this.hus = hus;
	}
	@Override
	public String toString() {
		return "Wife [id=" + id + ", name=" + name + ", hus=" + hus + "]";
	}
	
}
