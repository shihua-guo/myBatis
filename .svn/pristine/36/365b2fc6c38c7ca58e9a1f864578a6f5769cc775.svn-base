package com.briup.many2one;

public class MStudent {
	private int id;
	private String name;
	public MStudent (){}
	public MStudent(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	@Override
	public String toString() {
		return "MStudent [id=" + id + ", name=" + name + "]";
	}
	
}
